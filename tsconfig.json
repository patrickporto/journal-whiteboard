{
    "compilerOptions": {
        "target": "es2018",
        "rootDir": "src",
        "outDir": "dist",
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "strict": false,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "jsx": "react",
        "paths": {
            "*": [ "./src/types/*"]
        }
    }
}
